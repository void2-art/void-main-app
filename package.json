{
  "name": "void-main-app",
  "version": "1.0.0",
  "description": "TypeScript application for Raspberry Pi 5 with sensor integration, display output, and AI services",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "raspberry-pi",
    "sensors",
    "typescript",
    "iot",
    "chatgpt",
    "elevenlabs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "ws": "^8.13.0",
    "openai": "^4.20.1",
    "axios": "^1.5.0",
    "raspi": "^6.0.1",
    "raspi-gpio": "^6.0.1",
    "raspi-i2c": "^6.0.1",
    "raspi-spi": "^6.0.1",
    "raspi-pwm": "^6.0.1",
    "raspi-serial": "^6.0.1",
    "johnny-five": "^2.1.0",
    "raspi-io": "^10.0.1",
    "canvas": "^2.11.2",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.10.0",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/ws": "^8.5.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/jest": "^29.5.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/void-main-app.git"
